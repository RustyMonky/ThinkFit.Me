<h1><%= link_to team.name, team_path(team) %></h1>
<p>

<div id="team-description">
  Description: <%= team.description %>
</div>
<br/>
<div id="team-user">
  Created by: <%= User.find(team.owner_id).name %>
</div>
</p>
<br/>

<div class="roster">
  <h3>Members</h3>
  <div class="roster">

      <% if team.users != [] %>
          <p>SPACEFILLER</p>
          <div class="person">

          </div>
      <% else %>
      <% end %>

    <p><%= button_to "Join!", team_join_path(team.id) %></p>  
  </div>
</div>